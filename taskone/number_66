
func plusOne(digits []int) []int {
    if len(digits)==0{
		return append(digits,[]int{1}...)
	}
	var lens =  len(digits)
	var jw = 1
	for i:=lens-1 ;i>=0 ; i--{
		if jw==0{
			return digits
		}
		if digits[i]+jw==10 {
			if i==0 {
				digits[i] = 0
				return append([]int{1},digits...)
			}
			digits[i] = 0
			jw=1
		}else{
			digits[i] +=1
			jw=0
		}
	}
	return digits
}
